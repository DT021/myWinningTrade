<script type='text/x-handlebars' data-template-name='graph'>
<div class="chart_wrapper">
  {{#if stocksIsEmpty}}
    <progress></progress>
  {{else}}
    <div class="btn-group">
      <button {{bindAttr class="buttonHistoricalClasses"}} {{action toggleCurrentPeriod target="controller"}} data-period='historical'>Historical</button>
      <button {{bindAttr class="buttonIntradayClasses"}} {{action toggleCurrentPeriod target="controller"}} data-period='intraday'>Intraday</button>
    </div>
  {{/if}}


 <div class="chart"></div>

</div>
</script>
